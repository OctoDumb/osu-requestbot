{
  "name": "osu-requestbot",
  "version": "1.0.0",
  "description": "An Electron-based osu! request bot for Twitch",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "./node_modules/.bin/electron-builder build --x64",
    "build32": "./node_modules/.bin/electron-builder build --ia32"
  },
  "author": "OctoDumb",
  "license": "ISC",
  "build": {
    "appId": "org.OctoDumb.osuRequestBot",
    "extraFiles": [
      "public"
    ],
    "win": {
      "target": [
        "NSIS"
      ]
    }
  },
  "devDependencies": {
    "electron": "^4.0.3",
    "electron-builder": "^20.38.5"
  },
  "dependencies": {
    "express": "^4.16.4",
    "node-osu": "^1.3.2",
    "ojsama": "^1.0.19",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "tmi.js": "^1.3.0",
    "youtube-live-chat": "^1.0.3"
  }
}
